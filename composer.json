{
    "name": "heymoon/memcached-lib",
    "description": "Library for memcached access",
    "type": "library",
    "require": {
        "php": "^8.1"
    },
    "scripts": {
        "test": [
            "@composer install --ignore-platform-req=ext-tokenizer --ignore-platform-req=ext-dom --ignore-platform-req=ext-ctype --ignore-platform-req=ext-xmlwriter --ignore-platform-req=ext-xml",
            "phpunit"
        ],
        "test-junit": [
            "@composer install --ignore-platform-req=ext-tokenizer --ignore-platform-req=ext-dom --ignore-platform-req=ext-ctype --ignore-platform-req=ext-xmlwriter --ignore-platform-req=ext-xml",
            "phpunit --log-junit=runtime/tests.xml"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "HeyMoon\\MemcachedLib\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HeyMoon\\MemcachedLib\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Egor",
            "email": "me@heymoon.cc"
        }
    ]
}
